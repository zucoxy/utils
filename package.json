    "bumpp": "^8.2.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^4.9.3",
