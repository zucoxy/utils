{
  "author": "unyu <1078119795@qq.com>",
  "bugs": {
    "url": "https://github.com/zucoxy/utils/issues"
  },
  "description": "My Commonly used JS/TS utils",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@unyu/eslint-config": "^0.1.9",
    "bumpp": "^8.2.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "homepage": "https://github.com/zucoxy/utils#readme",
  "keywords": [
    "utils"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "name": "@unyu/utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zucoxy/utils.git"
  },
  "scripts": {
    "dev": "tsup src/index.ts",
    "build": "tsup",
    "start": "esno src/index.ts",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write .",
    "release": "bumpp --commit --push --tag && npm publish --access public"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "version": "0.0.5"
}
